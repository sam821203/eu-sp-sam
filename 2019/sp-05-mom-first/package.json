{
  "name": "sp-mom-first",
  "version": "1.0.0",
  "description": "母親最大牌",
  "private": true,
  "scripts": {
    "prestart": "rm -rf ./dist/develop/",
    "start": "parcel src/pug/index.pug --open -d ./dist/develop/",
    "prebuild": "rm -rf ./dist/release/",
    "build": "parcel build src/pug/index.pug --no-content-hash --no-source-maps -d dist/release/ --public-url ./"
  },
  "keywords": [
    "母親",
    "最大牌",
    "etmall",
    "u-mall",
    "東森購物",
    "森森購物"
  ],
  "author": "JackDou",
  "license": "ISC",
  "devDependencies": {
    "flexibility": "^2.0.1",
    "parcel-bundler": "^1.12.3",
    "pug": "^2.0.3",
    "sass": "^1.17.3"
  },
  "dependencies": {
    "jquery": "^3.4.0",
    "slick-carousel": "^1.8.1",
    "swiper": "^4.5.0"
  },
  "browserslist": [
    "defaults",
    "IE >= 9"
  ]
}
