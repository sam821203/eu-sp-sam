{
  "name": "3c-lang-live",
  "version": "1.0.0",
  "description": "3c浪live輪播影片頁",
  "main": "src/pug/index.pug",
  "scripts": {
    "prestart": "rm -rf ./dist/develop/*.*",
    "start": "parcel src/pug/index.pug --open -d ./dist/develop/",
    "prebuild": "rm -rf ./dist/release/*.*",
    "build": "parcel build src/pug/index.pug --no-source-maps --no-content-hash -d dist/release/ --public-url ./"
