  "devDependencies": {
    "jquery": "^3.4.1",
    "parcel-bundler": "^1.12.2",
    "pug": "^2.0.3",
    "sass": "^1.17.3"
  },
  "dependencies": {
    "slick-carousel": "^1.8.1",
    "swiper": "^4.5.0"
  }
}
