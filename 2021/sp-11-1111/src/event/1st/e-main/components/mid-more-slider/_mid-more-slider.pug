section.promo__group.promo__group--mid-more.m-bottom--md(data-title="品牌大賞")
  .layout--main
    .promo__wrap
      .promo__zone
        .decoration__box#MidMoreSliderTitle
          img(src="event/1st/e-main/components/mid-more-slider/title-mid-more-slider.png", alt="")
      #sliderMidMore.promo__slider.swiper-container
        ul.swiper-wrapper
          each data in midMoreData
            li.swiper-slide
              a(href= data.link, title= data.alt)
                img.swiper-lazy(data-src= data.imgSrc, alt= data.alt)
                .swiper-lazy-preloader.swiper-lazy-preloader-white
                .text__box
                  span= data.text

