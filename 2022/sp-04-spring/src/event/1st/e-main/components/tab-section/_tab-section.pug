              li.tab__item(title="家電")
                img(src="event/1st/e-main/components/tab-section/items/tab/tab-6.png", alt="家電")
                .tab__item--top
                  img(src="event/1st/e-main/components/tab-section/items/tab/tab-6-2.png", alt="家電")
          li.content__zone
            ul#tabContentBlock.content__block
              li.content__box.active
                .item__wrap
                  each data in tabContent1
