section.promo__group.promo__group--tab-section.m-bottom--lg(data-title="品牌推薦")
  .layout--main
    .decor__wrap
      .decor__item#decorTabSection
        img(src="event/1st/e-main/components/tab-section/decor-tab-flower.png", alt="")
      .decor__item#decorTabSection2
        img(src="event/1st/e-main/components/tab-section/decor-tab-flower-2.png", alt="")
    .promo__wrap
      .promo__zone 
        ul.tab__wrap
          li.tab__zone
            ul#tabBox.tab__box
              li.tab__item(title="美妝")
                img(src="event/1st/e-main/components/tab-section/items/tab/tab-1.png", alt="美妝")
                .tab__item--top.active
                  img(src="event/1st/e-main/components/tab-section/items/tab/tab-1-2.png", alt="美妝")
              li.tab__item(title="生活")
                img(src="event/1st/e-main/components/tab-section/items/tab/tab-2.png", alt="生活")
                .tab__item--top
                  img(src="event/1st/e-main/components/tab-section/items/tab/tab-2-2.png", alt="生活")
              li.tab__item(title="3C")
                img(src="event/1st/e-main/components/tab-section/items/tab/tab-3.png", alt="3C")
                .tab__item--top
                  img(src="event/1st/e-main/components/tab-section/items/tab/tab-3-2.png", alt="3C")
              li.tab__item(title="保健")
                img(src="event/1st/e-main/components/tab-section/items/tab/tab-4.png", alt="保健")
                .tab__item--top
                  img(src="event/1st/e-main/components/tab-section/items/tab/tab-4-2.png", alt="保健")
              li.tab__item(title="流行")
                img(src="event/1st/e-main/components/tab-section/items/tab/tab-5.png", alt="流行")
