section.promo__group.promo__group--tab-section.m-bottom--lg(data-title="大牌鉅獻")
  .layout--main
    .promo__wrap
      .promo__zone 
        .tab__wrap
          .tab__zone
            ul#tabBoxSecond.tab__box
              li.tab__item.active(title="聯華食品")
                img(src="event/1st/e-main/components/tab-section/tabs/tab-1.png", alt="聯華食品")
              li.tab__item(title="聯華食品")
                img(src="event/1st/e-main/components/tab-section/tabs/tab-2.png", alt="聯華食品")
              li.tab__item(title="聯華食品")
                img(src="event/1st/e-main/components/tab-section/tabs/tab-3.png", alt="聯華食品")
              li.tab__item(title="聯華食品")
                img(src="event/1st/e-main/components/tab-section/tabs/tab-4.png", alt="聯華食品")
              li.tab__item(title="聯華食品")
                img(src="event/1st/e-main/components/tab-section/tabs/tab-5.png", alt="聯華食品")
              li.tab__item(title="聯華食品")
                img(src="event/1st/e-main/components/tab-section/tabs/tab-6.png", alt="聯華食品")
          .content__zone
            ul#tabContentBlockSecond.content__block
              each data, index in tabSectionData
                li.content__box.active
                  .tab__slider
                    each item, itemIndex in  data
                      .content__item
                        a(href=  item.link, title= item.alt)
                          img(src=  item.imgSrc,  item.alt)

