section.promo__group.promo__group--w3m3(data-title="領券現折")
  .layout--main
    .decor__wrap.full-parent
      .decor__item.decor__item--title
        img(src="event/1st/e-main/components/event-w3m3/decor-event-w3m3-title.png", alt="")
    .promo__wrap
      .promo__zone
        .promo__block
          #w3m3Wrap.event__wrap
            ul.good__wrap
              each data, index in w3m3Data
                li
                  a(href= data.link, title= data.alt)
                    .ice-shine
                    - var hasImg = data.imgSrc ? true : false
                    if (hasImg)
                      img(src= data.imgSrc, alt= data.alt)