section.promo__group.promo__group--tab-section.m-bottom--lg(data-title="大牌鉅獻")
  .layout--main
    .decor__wrap
      .decor__item.decor__item--title
        img(src="event/2nd/e-main/components/tab-section/decor-tab-title.png" alt="")
      .decor__item.decor__item--swan#decorTabSectionSwan
        img(src="event/2nd/e-main/components/tab-section/decor-tab-swan.png" alt="")
      .decor__item.decor__item--ball#decorTabSectionBall
        img(src="event/2nd/e-main/components/tab-section/decor-tab-ball.png" alt="")
      .decor__item.decor__item--left#decorTabSectionLeft
        img(src="event/2nd/e-main/components/tab-section/decor-grass-left.png" alt="")
      .decor__item.decor__item--right#decorTabSectionRight
        img(src="event/2nd/e-main/components/tab-section/decor-grass-right.png" alt="")
    .promo__wrap
      .promo__zone 
        ul.tab__wrap
          li.tab__zone
            ul#tabBox.tab__box
              li.tab__item(title="美妝")
                img(src="event/2nd/e-main/components/tab-section/items/tab/tab-1.png" alt="美妝")
                .tab__item--top.active
                  img(src="event/2nd/e-main/components/tab-section/items/tab/tab-1-2.png" alt="美妝")
              li.tab__item(title="保健")
                img(src="event/2nd/e-main/components/tab-section/items/tab/tab-2.png" alt="保健")
                .tab__item--top
                  img(src="event/2nd/e-main/components/tab-section/items/tab/tab-2-2.png" alt="保健")
              li.tab__item(title="大家電")
                img(src="event/2nd/e-main/components/tab-section/items/tab/tab-3.png" alt="大家電")
                .tab__item--top
                  img(src="event/2nd/e-main/components/tab-section/items/tab/tab-3-2.png" alt="大家電")
              li.tab__item(title="通訊")
                img(src="event/2nd/e-main/components/tab-section/items/tab/tab-4.png" alt="通訊")
                .tab__item--top
                  img(src="event/2nd/e-main/components/tab-section/items/tab/tab-4-2.png" alt="通訊")
              li.tab__item(title="美食")
                img(src="event/2nd/e-main/components/tab-section/items/tab/tab-5.png" alt="美食")
                .tab__item--top
                  img(src="event/2nd/e-main/components/tab-section/items/tab/tab-5-2.png" alt="美食")
              li.tab__item(title="傢寢-J")
                img(src="event/2nd/e-main/components/tab-section/items/tab/tab-6.png" alt="傢寢-J")
                .tab__item--top
                  img(src="event/2nd/e-main/components/tab-section/items/tab/tab-6-2.png" alt="傢寢-J")
          li.content__zone
            ul#tabContentBlock.content__block
              li.content__box.active
                ul.item__wrap
                  li.swiper-slide
                    a(href="https://www.etmall.com.tw/c3/46340") 
                      img(src="event/2nd/e-main/components/tab-section/items/item/tab-item-1-1.png" alt="夏日狂歡5折up贈面膜")
              li.content__box
                ul.item__wrap
                  li.swiper-slide
                    a(href="https://www.etmall.com.tw/p/178990") 
                      img(src="event/2nd/e-main/components/tab-section/items/item/tab-item-2-1.png" alt="夏日保健大作戰滿額贈好禮")
              li.content__box
                ul.item__wrap
                  li.swiper-slide
                    a(href="https://media.etmall.com.tw/sp/10222/m/index.html")
                      img(src="event/2nd/e-main/components/tab-section/items/item/tab-item-3-1.png" alt="滿額登記送")
              li.content__box
                ul.item__wrap
                  li.swiper-slide
                    a(href="https://media.etmall.com.tw/sp/11387/m/index.html")
                      img(src="event/2nd/e-main/components/tab-section/items/item/tab-item-4-1.png" alt="小米旗艦館")
              li.content__box
                ul.item__wrap
                  li.swiper-slide
                    a(href="https://www.etmall.com.tw/bs/162580")
                      img(src="event/2nd/e-main/components/tab-section/items/item/tab-item-5-1.png" alt="滿額登記送可口可樂 手拉購物車")
              li.content__box
                ul.item__wrap
                  li.swiper-slide
                    a(href="https://www.etmall.com.tw/gp/177853")
