- var promoData1 = promoData1

section.promo__group.promo__group--slider-w19-2.m-bottom--lg(data-title="假如有標題")
  .layout--main
    .promo__wrap
      //- .decor__wrap.full-parent
      //-   .decor__item.decor__item--light
      //-     img(src="event/1st/e-main/components/slider-w19-2/decor-slider-w19-2.png", alt="")
      .promo__zone
        .promo__block
          .promo__slide--parent#dynamicSlide2.swiper-container.full-parent
            ul.swiper-wrapper
              each data, index in promoData1
                li.swiper-slide
                  .slide__box
                    .slide__box--bn
                      a(href= data.link, alt= data.alt)
                        img(src= data.imgSrc, alt= data.alt)
                    .slide__box--item
                      - var dataChild = data.child
                      each child, index in dataChild
                        .item__box
                          a(href= child.link)
                            img(src= child.imgSrc, alt= child.alt)
            .swiper-button-next
              img(src="event/1st/e-main/components/slider-w19-2/arrow-2.png", alt="")
            .swiper-button-prev
              img.img-reverse-x(src="event/1st/e-main/components/slider-w19-2/arrow-2.png", alt="")










